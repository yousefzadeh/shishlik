-- dbo.TenantAccess_HubToSpokes source

CREATE VIEW dbo.TenantAccess_HubToSpokes
                WITH SCHEMABINDING
                AS
                SELECT 
                    t.ServiceProviderId AS RequestingTenantId,
                    t.Id AS AccessibleTenantId
                FROM dbo.AbpTenants t
                WHERE t.ServiceProviderId IS NOT NULL;


