-- dbo.secureAssessment source

CREATE VIEW dbo.secureAssessment
AS
    SELECT 
        *
    FROM 
        Assessment a
    WHERE 
        EXISTS(SELECT 1 FROM [dbo].[fn_securitypredicate](a.TenantId, CAST(SESSION_CONTEXT(N'TenantID') as INT)))
WITH CHECK OPTION;


